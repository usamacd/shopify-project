<div class="product">
  <h1>{{ product.title }}</h1>
  
  <p>{{ product.description }}</p>
  
  {% for image in product.images %}
  {{ image | img_url: "200x" | img_tag }}
  
  {% endfor %}<br> 
   
  {{ product.price | money }}<br>
  {{ product.handle }}

   {% for variant in product.variants %}
  
  {{ variant.title }}<br>
  {{ variant.price | money_without_currency  }}
  

  {% endfor %}
