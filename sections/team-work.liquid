<section class="ap-team-imgs page-sec pb-0">
        <header class="page-sec-header">
          {% if section.settings.heading != blank %}
          <h2>{{ section.settings.heading }}</h2>
          {% endif %}
        </header>
        <div class="">
            <div class="ap-img-hide-tab-mobile text-center">
                {% if section.settings.select_image != blank %}
                  <img class="img-fluid" src="{{ section.settings.select_image | img_url: "1440x"}}" />
                {% endif %}
            </div>
        </div>
</section>

 {% schema %}
  {
    "name": "team work",
    "settings": [
      {
          "type":"text",
          "id":"heading",
          "label":"heading"
      },
        {
            "type":"image_picker",
            "id":"select_image",
            "label":"Select image"
        }
            ]
    }
  {% endschema %}